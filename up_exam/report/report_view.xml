                                                <td class="marks-cell">
                                                    <!--                                                <t t-esc="sub.subject_id.name"/>-->
                                                    <t t-esc="sub.mo1"/>
                                                    +
                                                    <t t-esc="sub.mo2"/>
                                                    +
                                                    <span style="color:red">
                                                        <t t-esc="sub.grace_mark"/>
                                                    </span>
                                                    =
                                                    <t t-esc="sub.total_mark"/>
                                                    <!--                                                <t t-esc="stud.get_subject_mark(sub.id)"/>-->
                                                </td>

                                            </t>
                                        </tr>
                                    </t>


                                </tbody>
                            </table>
                        </div>
                    </div>

